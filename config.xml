<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets" id="https://addons.opera.com/addons/extensions/details/traein/" version="0.3" viewmodes="minimized">
	<name>Traein</name>
	<feature name="opera:speeddial">
	<param name="url" value="http://www.irishrail.ie"/>
	</feature>
	<icon src="ie.png"/>
	<access origin="http://api.irishrail.ie"/>
	<preference name="feedUrl" value="http://api.irishrail.ie/realtime/realtime.asmx/getStationDataByCodeXML?StationCode="/>
	<preference name="count" value="6"/>
	<preference name="interval" value="60"/>
	<preference name="direction" value=""/>
	<preference name="stationCode" value=""/>
	<author href="http://smallroomstudios.net">Smallroomstudios.net</author>
	<description xml:lang="en">Get realtime timetable information for Irish Rail stations. Source: https://github.com/paddy2k/Speeddial-Traein</description>
</widget>
