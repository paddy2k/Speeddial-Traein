{
"name": "Traein",
"developer": {"name" : "paddy2k", "url" : "http://smallroomstudios.net"},
"description" : "Get realtime timetable information for Irish Rail stations. Source: https://github.com/paddy2k/Speeddial-Traein",
"manifest_version" : 2,
"version" : "0.4",
"background" : {"page" : "index.html"},
"icons" : {"128": "ie.png"},
"options_page" : "options.html",
"web_accessible_resources" : ["AUTHORS","LICENCE","README","ie.png","options.bg.png","stations.js","LICENSE.TXT"],
"permissions" : ["https://*/*", "storage", "http://*/*"],
"speeddial" : {"url": "http://www.irishrail.ie"},
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'unsafe-eval';"
}
